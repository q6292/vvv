local Players = game:GetService("Players")
local player = Players.LocalPlayer
local PlayerGui = player:WaitForChild("PlayerGui")

-- Main GUI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "DiscordPrompt"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = PlayerGui

-- Main Frame
local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 420, 0, 240)
Frame.Position = UDim2.new(0.5, -210, 0.5, -120)
Frame.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
Frame.BorderSizePixel = 0
Frame.ClipsDescendants = true
Frame.Parent = ScreenGui

-- UICorner
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 10)
UICorner.Parent = Frame

-- Draggable
Frame.Active = true
Frame.Draggable = true

-- Title
local Title = Instance.new("TextLabel")
Title.Text = "üîí Get Key Access"
Title.Font = Enum.Font.GothamBold
Title.TextSize = 24
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.BackgroundTransparency = 1
Title.Size = UDim2.new(1, 0, 0, 50)
Title.Position = UDim2.new(0, 0, 0, 10)
Title.Parent = Frame

-- Message
local Message = Instance.new("TextLabel")
Message.Text = "You must join our Discord to get the key!"
Message.Font = Enum.Font.Gotham
Message.TextSize = 18
Message.TextColor3 = Color3.fromRGB(200, 200, 200)
Message.BackgroundTransparency = 1
Message.TextWrapped = true
Message.Size = UDim2.new(1, -40, 0, 60)
Message.Position = UDim2.new(0, 20, 0, 60)
Message.Parent = Frame

-- Copy Confirmation
local Confirm = Instance.new("TextLabel")
Confirm.Text = ""
Confirm.Font = Enum.Font.Gotham
Confirm.TextSize = 16
Confirm.TextColor3 = Color3.fromRGB(0, 255, 0)
Confirm.BackgroundTransparency = 1
Confirm.Size = UDim2.new(1, -20, 0, 20)
Confirm.Position = UDim2.new(0, 10, 0, 130)
Confirm.Parent = Frame

-- Copy Invite Button
local CopyBtn = Instance.new("TextButton")
CopyBtn.Text = "Copy Discord Invite"
CopyBtn.Font = Enum.Font.GothamBold
CopyBtn.TextSize = 18
CopyBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
CopyBtn.BackgroundColor3 = Color3.fromRGB(65, 105, 225)
CopyBtn.Size = UDim2.new(0.45, -10, 0, 40)
CopyBtn.Position = UDim2.new(0, 20, 1, -60)
CopyBtn.Parent = Frame

local CopyUICorner = Instance.new("UICorner", CopyBtn)
CopyUICorner.CornerRadius = UDim.new(0, 8)

-- I've Joined Button
local JoinBtn = Instance.new("TextButton")
JoinBtn.Text = "I've Joined"
JoinBtn.Font = Enum.Font.GothamBold
JoinBtn.TextSize = 18
JoinBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
JoinBtn.BackgroundColor3 = Color3.fromRGB(30, 144, 255)
JoinBtn.Size = UDim2.new(0.45, -10, 0, 40)
JoinBtn.Position = UDim2.new(0.55, 10, 1, -60)
JoinBtn.Parent = Frame

local JoinUICorner = Instance.new("UICorner", JoinBtn)
JoinUICorner.CornerRadius = UDim.new(0, 8)

-- Copy Invite Logic
CopyBtn.MouseButton1Click:Connect(function()
	local success, err = pcall(function()
		setclipboard("https://discord.gg/NMwq5ghcsF")
	end)

	if success then
		Confirm.Text = "‚úÖ Invite copied to clipboard!"
	else
		Confirm.Text = "‚ùå Failed to copy invite."
	end

	task.delay(3, function()
		Confirm.Text = ""
	end)
end)

-- Continue Button Logic
JoinBtn.MouseButton1Click:Connect(function()
	ScreenGui:Destroy()
	loadstring(game:HttpGet("https://pastefy.app/gDbxZw7a/raw"))()
end)
